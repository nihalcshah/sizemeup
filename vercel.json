{
    "builds": [
      {
        "src": "sizemeup/wsgi.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "sizemeup/wsgi.py"
      }
    ],
    "functions": {
        "api/**/*.py": {
          "excludeFiles": "public/**"
        }
      }
    
  }